{
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "jest": true
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
    "plugins": ["react", "prettier"],
    "rules": {
      "prettier/prettier": ["error"],
      "react/react-in-jsx-scope": "off" 
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
  